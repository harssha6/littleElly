    height: 100%;
    flex-direction: column;
    font-size: 1.25rem;
    color: #8a60d2;
    font-weight: 800;
    text-align: center;
    text-transform: uppercase;
    letter-spacing: 2px;
    line-height: 120%;
    padding: 50px 20px;
    align-items: center;
  }
  

  .card.brown{
    border: 2px dashed var(--Orange, #f6bd60);
    color: #ae7e30;
    background: linear-gradient(180deg, #fff 0%, #fff3de 100%);
  }
  .card.green{
