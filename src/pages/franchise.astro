    background: linear-gradient(180deg, #fff 0%, #f0ffe2 100%);
  }
  .partner-container{
    display: grid;
    grid-template-columns: repeat(3,1fr);
    place-items: center;
    gap: 2rem;
    column-gap: 3.5rem;
  }
  .start-franchise-container{
    width: 80%;
    display: grid;
    gap: 2rem;
    margin-top: 1rem;
    grid-template-columns: repeat(3,1fr);
  }
  /* tablet and phone */
  @media (max-width: 1080px) {
    .partner-container{

        place-items: center;
        grid-template-columns: repeat(2,1fr);
    }
    .start-franchise-container{
      width: 40%;

      grid-template-columns: repeat(2,1fr);
    }
  }
  @media (max-width: 700px) {
